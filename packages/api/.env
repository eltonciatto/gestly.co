# Server
PORT=3000
NODE_ENV=development

# Database
POSTGRES_URL=postgres://postgres:password@localhost:5432/gestly

# JWT
JWT_SECRET=gestly-dev-jwt-secret-key-min-32-chars-long
JWT_EXPIRES_IN=1h

# Redis
REDIS_URL=redis://localhost:6379

# CORS
CORS_ORIGIN=http://localhost:5173

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=60000

# Logging
LOG_LEVEL=info